@*@model WebApplicationSamplePartialView.PartialView*@
<h2>
    Student Details From partial View
</h2>

<table style="font-family:Arial; border:1px solid; width:auto">
    <tr>
        <th>
            Name
        </th>
        <th>
            Gender
        </th>
        <th>
            City
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>

            <td>
                @item.Gender
            </td>
            <td>
                @item.City
            </td>
        </tr>
    }

</table>

